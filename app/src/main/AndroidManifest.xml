<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="thememeteam.com.yummycrummyapp4" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="thememeteam.com.yummycrummyapp4.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="thememeteam.com.yummycrummyapp4.Login_Activity"
            android:label="@string/title_activity_login_" >
            <intent-filter>
                <action android:name="thememeteam.com.yummycrummyapp4.Login_Activity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="thememeteam.com.yummycrummyapp4.CreateNewAccount"
            android:label="@string/title_activity_create_new_account" >
            <intent-filter>
                <action android:name="thememeteam.com.yummycrummyapp4.CreateNewAccount" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="thememeteam.com.yummycrummyapp4.TestDB"
            android:label="@string/title_activity_test_db" >
            <intent-filter>
                <action android:name="thememeteam.com.yummycrummyapp4.TestDB" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
